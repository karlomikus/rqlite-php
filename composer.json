{
    "name": "karlomikus/rqlite",
    "autoload": {
        "psr-4": {
            "Kami\\Rqlite\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Karlo Miku≈°",
            "email": "karlo.mikus1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "To use the Guzzle adapter",
        "ext-curl": "To use the Curl adapter"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "symfony/var-dumper": "^7.1",
        "phpstan/phpstan": "^1.12",
        "guzzlehttp/guzzle": "^7.0",
        "pestphp/pest": "^3.5",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
